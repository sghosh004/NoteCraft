  <body class="bg-dark">
    <div class="container mse-5">
      <!-- <h1 class="text-center">View Document</h1> -->
      <div class="d-flex justify-content-center">
        <iframe
          src="/documents/<%= documentUrl %>"
          width="100%"
          height="600px"
          frameborder="0"
        ></iframe>
      </div>
      <div class="text-center mt-3">
        <a href="/" class="btn btn-primary">Back to Home</a>
      </div>
    </div>
