<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Search Results</title>
  </head>
  <body>
    <h1>Search Results</h1>
    <ul>
      <% files.forEach(file => { %>
      <li>
        <strong><%= file.subjectName %> - <%= file.year %></strong>
        (<%= file.type %>) -
        <a href="/uploads/<%= file.filename %>"><%= file.originalName %></a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
